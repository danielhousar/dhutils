LIBDIR=$(PREFIX)/lib
INCLUDEDIR=$(PREFIX)/include
SYSCONFDIR=/etc

CC=gcc
CFLAGS=-march=native -mtune=native -O3 -Wall
DEFS=
LIBDEFS=
LDFLAGS=

E = @echo
Q = @

OUTOBJDIR=../.out
OUTBINDIR=../.bin
OUTLIBDIR=../.lib

# HERE you can modify, what will be built
SUBDIRS=libdh quadequi datatypes portping initmount

LIBDHPARTS=math str misc memset fun

LIBDHOBJS=$(OUTOBJDIR)/math.o \
	$(OUTOBJDIR)/str.o \
	$(OUTOBJDIR)/misc.o \
	$(OUTOBJDIR)/memset.o \
	$(OUTOBJDIR)/fun.o

